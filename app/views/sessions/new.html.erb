<!-- Login Page -->
<% provide(:title, "Login") %>
<div class="center jumbotron">
  <h1>Login</h1>

  <%= form_tag '/login' do%>
    <%= label_tag :username%><br>
    <%= text_field_tag :username%><br>
    <%= label_tag :password%><br>
    <%= password_field_tag :password%><br>
    <%= puts(@error) %><br>
    <%= submit_tag "Login", class: "btn btn-lg btn-success" %><br>
  <%end%>
</div>
